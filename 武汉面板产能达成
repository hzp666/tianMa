select sum(produce_quantity_done_quantity) as produce_done_quantity ,sum(produce_quantity_base_daily) as produce_planed_quantity ,sum(produce_quantity_done_quantity)/sum(produce_quantity_base_daily) as produce_quantity_rate,"武汉G4.5" as produce_quantity_location,"09月" as produce_quantity_plan_month  from  quantity_day_report where  produce_quantity_factory ="面板段" and produce_quantity_location="武汉G4.5" and produce_quantity_plan_month = "09月"
